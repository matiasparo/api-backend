{
	"name": "api-backend",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"postinstall": "npx tsc",
		"build": "npm run clean && npx tsc && npm run lint",
		"clean": "rimraf ./dist",
		"lint": "eslint \"./src/**/*.{js,ts,tsx}\" --quiet --fix",
		"tsc": "tsc",
		"dev": "npm run clean && tsc-watch --onSuccess \"node ./dist/app.js\"",
		"test": "jest",
		"test_coverage": "jest --coverage --verbose",
		"start_pm2": "npx pm2 start pm2.yml",
		"start": "node dist/app.js",
		"migration": "node node_modules/db-migrate/bin/db-migrate --config migration-config.json",
		"migration:create": "npm run migration create",
		"migration:run": "npm run migration up",
		"migration:rollback": "npm run migration down",
		"migration:reset": "npm run migration reset"
	},
	"author": "Matias",
	"dependencies": {
		"@types/adm-zip": "^0.4.33",
		"@types/exceljs": "^1.3.0",
		"adm-zip": "^0.4.14",
		"aws-sdk": "^2.635.0",
		"bcrypt": "^3.0.6",
		"body-parser": "^1.19.0",
		"class-validator": "^0.11.0",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"db-migrate": "^0.11.11",
		"db-migrate-mysql": "^2.1.1",
		"dotenv": "^8.0.0",
		"exceljs": "^4.0.1",
		"express": "^4.17.1",
		"helmet": "^3.21.2",
		"intl-dateformat": "^0.1.4",
		"jsonwebtoken": "^8.5.1",
		"module-alias": "^2.2.2",
		"moment": "^2.24.0",
		"mysql": "^2.18.1",
		"pm2": "^4.2.3",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^6.5.5",
		"sanitize": "^2.1.0",
		"sanitize-filename": "^1.6.3",
		"typeorm": "^0.2.22",
		"winston": "^3.2.1"
	},
	"devDependencies": {
		"@types/aws-sdk": "^2.7.0",
		"@types/bcrypt": "^3.0.0",
		"@types/body-parser": "^1.17.0",
		"@types/compression": "^1.0.1",
		"@types/cors": "^2.8.6",
		"@types/dotenv": "^6.1.1",
		"@types/express": "^4.17.0",
		"@types/helmet": "0.0.45",
		"@types/jest": "^25.1.1",
		"@types/jsonwebtoken": "^8.3.3",
		"@types/moment": "^2.13.0",
		"@types/node": "^12.7.2",
		"@types/winston": "^2.4.4",
		"@typescript-eslint/eslint-plugin": "^2.32.0",
		"@typescript-eslint/parser": "^2.32.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-node": "^9.1.0",
		"eslint-plugin-prettier": "^3.1.3",
		"jest": "^25.1.0",
		"prettier": "^2.0.5",
		"rimraf": "^3.0.0",
		"ts-jest": "^25.2.0",
		"ts-node": "^8.3.0",
		"tsc-watch": "^4.1.0",
		"typescript": "^3.8.3",
		"typescript-eslint-parser": "^22.0.0"
	},
	"jest": {
		"verbose": true,
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	},
	"_moduleAliases": {
		"@components": "dist/components",
		"@utils": "dist/utils"
	}
}
